{
  "name": "pm2-bot",
  "version": "0.1.1",
  "description": "Template for creating future telegram bot apps",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "npm run generate && npm run deploy && tsc -p .",
    "dev": "nodemon src/index.ts",
    "generate": "prisma generate",
    "deploy": "prisma migrate deploy",
    "migrate": "prisma migrate dev",
    "reset": "prisma migrate reset"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/francisyzy/pm2-bot.git"
  },
  "author": "francisyzy",
  "bugs": {
    "url": "https://github.com/francisyzy/pm2-bot/issues"
  },
  "homepage": "https://github.com/francisyzy/pm2-bot#readme",
  "engines": {
    "node": "17.x"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "qrcode-terminal": "^0.12.0",
    "telegraf": "^4.7.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.18",
    "@types/qrcode-terminal": "^0.12.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.5.0",
    "typegram": "^3.8.0",
    "typescript": "^4.5.5"
  }
}
